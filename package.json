{
  "name": "wenjuan-backend",
  "version": "0.0.1",
  "description": "",
  "author": "ygqygq2",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:hmr": "cross-env NODE_ENV=development nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --maxWorkers=1",
    "test:e2e:watch": "jest --config ./test/jest-e2e.json --maxWorkers=1 --watch",
    "typeorm": "typeorm -d ormconfig.js",
    "typeorm:dev": "typeorm-ts-node-commonjs -d ormconfig.ts",
    "migration:generate": "f() { npm run typeorm:dev migration:generate -p \"./src/migrations/$@\"; }; f",
    "migration:create": "npm run typeorm:dev migration:create",
    "migration:run:dev": "npm run typeorm:dev migration:run",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert:dev": "npm run typeorm:dev migration:revert",
    "migration:revert": "npm run typeorm migration:revert",
    "schema:drop": "npm run typeorm schema:drop",
    "prepush": "pnpm run lint",
    "postinstall": "simple-git-hooks",
    "preinstall": "npx only-allow pnpm",
    "commit": "git cz",
    "release": "bumpp"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "pnpm run lint",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@casl/ability": "^6.7.1",
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@nestjs/common": "^10.3.8",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.8",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.8",
    "@nestjs/platform-fastify": "^10.3.8",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/typeorm": "^10.0.2",
    "@types/uuid": "^9.0.8",
    "argon2": "^0.40.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "joi": "^17.13.0",
    "lodash": "^4.17.21",
    "mysql2": "3.9.7",
    "nest-winston": "^1.9.4",
    "nestjs-pino": "^4.0.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pino-http": "^9.0.0",
    "pino-roll": "1.1.0",
    "redlock": "5.0.0-beta.2",
    "reflect-metadata": "^0.2.2",
    "request-ip": "^3.3.0",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.1.1",
    "@jest/types": "^29.6.3",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.8",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.12",
    "@types/line-reader": "^0.0.37",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/request-ip": "^0.0.41",
    "@types/supertest": "^6.0.2",
    "@types/webpack-env": "^1.18.4",
    "@ygqygq2/eslint-config": "^1.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.1.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "jest": "29.7.0",
    "line-reader": "^0.4.0",
    "lint-staged": "^15.2.2",
    "pactum": "^3.6.7",
    "pino-pretty": "^11.0.0",
    "prettier": "^3.2.5",
    "run-script-webpack-plugin": "^0.2.0",
    "simple-git-hooks": "^2.11.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.7.1",
    "webpack": "^5.91.0",
    "webpack-node-externals": "^3.0.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "webpack"
      ]
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "preserveUnused": true
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
